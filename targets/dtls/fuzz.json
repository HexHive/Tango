{
	"exec": {
		"path": "./dtls",
		"args": ["./dtls"],
		"cwd": "./",
		"env": {"ASAN_OPTIONS": "detect_leaks=0"}
	},
	"channel": {
		"type": "udp",
		"udp": {
			"endpoint": "127.0.0.1",
			"port": 20220
		}
	},
	"loader": {
		"type": "replay",
		"forkserver": true,
		"disable_aslr": true
	},
	"input": {
		"type": "mutation"
	},
	"statemanager": {
		"type": "coverage",
		"strategy": "uniform"
	},
	"fuzzer": {
		"lib": "./lib/",
		"cwd": "./targets/dtls/",
		"workdir": "./workdir/"
	}
}
