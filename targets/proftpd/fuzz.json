{
    "driver": {
        "type": "unix",
        "exec": {
            "path": "./proftpd",
            "args": ["./proftpd", "-n", "-c", "./basic.conf", "-X"],
            "cwd": "./",
            "env": {"ASAN_OPTIONS": "abort_on_error=1:symbolize=0:detect_leaks=0"},
            "stdout": "inherit",
            "stderr": "inherit"
        },
		"disable_aslr": true,
		"forkserver": true
    },
	"loader": {
		"type": "replay"
	},
	"channel": {
		"type": "tcp",
        "endpoint": "127.0.0.1",
        "port": 2100,
		"fork_before_accept": true
	},
	"generator": {
		"type": "reactive"
	},
	"tracker": {
		"type": "coverage"
	},
	"strategy": {
		"type": "uniform"
	},
	"fuzzer": {
		"cwd": "./targets/proftpd/",
		"work_dir": "./workdir/",
		"seeds": "./seeds/",
		"resume": true
	}
}
