{
    "driver": {
        "type": "unix",
        "exec": {
            "path": "./pure-ftpd",
            "args": ["./pure-ftpd", "-S", "127.0.0.1,2100", "-A"],
            "cwd": "./",
            "env": {"ASAN_OPTIONS": "abort_on_error=1:symbolize=0:detect_leaks=0"}
        },
		"disable_aslr": true,
		"forkserver": true
    },
	"loader": {
		"type": "replay"
	},
	"channel": {
		"type": "tcp",
        "endpoint": "127.0.0.1",
        "port": 2100,
		"fork_before_accept": true
	},
	"generator": {
		"type": "reactive",
		"seeds": "./seeds/"
	},
	"tracker": {
		"type": "coverage"
	},
	"strategy": {
		"type": "uniform"
	},
	"fuzzer": {
		"cwd": "./targets/pureftpd/",
		"work_dir": "./workdir/",
		"resume": true
	}
}
