{
	"exec": {
		"path": "./live555",
		"args": ["./live555", "8554"],
		"cwd": "./",
		"env": {
			"ASAN_OPTIONS": "abort_on_error=1:symbolize=0:detect_leaks=0",
			"LD_PRELOAD": "../../lib/libfaketime/src/libfaketime.so.1",
			"FAKETIME": "+0 i0",
			"FAKETIME_NO_CACHE": 1
		}
	},
	"channel": {
		"type": "tcp",
		"tcp": {
			"endpoint": "127.0.0.1",
			"port": 8554
		}
	},
	"loader": {
		"type": "replay",
		"forkserver": false,
		"disable_aslr": true
	},
	"input": {
		"type": "reactive"
	},
	"statemanager": {
		"type": "coverage",
		"strategy": "uniform"
	},
	"fuzzer": {
		"cwd": "./targets/rtsp/",
		"lib": "./lib/",
		"seeds": "./seeds/",
		"workdir": "./workdir/",
		"resume": true
	}
}
