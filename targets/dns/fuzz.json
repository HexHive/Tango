{
	"exec": {
		"path": "./dnsmasq",
		"args": ["./dnsmasq", "-C", "dnsmasq.conf"],
		"cwd": "./",
		"env": {"ASAN_OPTIONS": "abort_on_error=1:symbolize=0:detect_leaks=0"}
	},
	"channel": {
		"type": "udp",
		"udp": {
			"endpoint": "127.0.0.1",
			"port": 5353
		}
	},
	"loader": {
		"type": "replay",
		"forkserver": true,
		"disable_aslr": true
	},
	"input": {
		"type": "mutation"
	},
	"statemanager": {
		"type": "coverage",
		"strategy": "uniform"
	},
	"fuzzer": {
		"lib": "./lib/",
		"cwd": "./targets/dns/",
		"workdir": "./workdir/",
		"resume": true
	}
}
