{
	"exec": {
		"path": "./targets/tcp_server/tcp_server",
		"args": ["./targets/tcp_server/tcp_server"],
		"env": {
			"ASAN_OPTIONS": "detect_leaks=0"
		},
		"stdout": "./stdout",
		"stderr": "./stderr"
	},
	"channel": {
		"type": "tcp",
		"tcp": {
			"endpoint": "127.0.0.1",
			"port": 5001
		}
	},
	"loader": {
		"type": "replay",
		"forkserver": true,
		"disable_aslr": true
	},
	"input": {
		"type": "mutation"
	},
	"statemanager": {
		"type": "coverage"
	},
	"fuzzer": {
		"lib": "./lib",
		"workdir": "./workdir"
	}
}
