{
	"exec": {
		"path": "./udp_server",
		"args": ["./udp_server"],
		"env": {
			"ASAN_OPTIONS": "detect_leaks=0"
		},
		"stdout": "./stdout",
		"stderr": "./stderr"
	},
	"channel": {
		"type": "udp",
		"udp": {
			"endpoint": "127.0.0.1",
			"port": 5001,
			"fork_location": "bind"
		}
	},
	"loader": {
		"type": "replay",
		"forkserver": true,
		"disable_aslr": true
	},
	"input": {
		"type": "mutation"
	},
	"statemanager": {
		"type": "coverage"
	},
	"fuzzer": {
		"cwd": "./targets/udp_server/",
		"lib": "./lib/",
		"workdir": "./workdir/",
		"resume": true
	}
}
