{
    "driver": {
        "type": "unix",
        "exec": {
            "path": "./daapd",
            "args": ["./daapd", "-d", "0", "-c", "forked-daapd.conf", "-f", "-w", "./htdocs"],
            "cwd": "./",
            "env": {
                "ASAN_OPTIONS": "abort_on_error=1:symbolize=0:detect_leaks=0",
                "LD_LIBRARY_PATH": "libevent_repo/build/lib/",
                "LD_PRELOAD": "../../lib/libfaketime/src/libfaketime.so.1",
                "FAKETIME": "+0 i0",
                "FAKETIME_NO_CACHE": "1"
            }
        },
		"disable_aslr": true,
		"forkserver": true
    },
	"loader": {
		"type": "replay"
	},
	"channel": {
		"type": "tcp",
        "endpoint": "127.0.0.1",
        "port": 3689,
		"fork_before_accept": true
	},
	"generator": {
		"type": "reactive"
	},
	"tracker": {
		"type": "coverage"
	},
	"strategy": {
		"type": "uniform"
	},
	"fuzzer": {
		"cwd": "./targets/daap/",
		"work_dir": "./workdir/",
		"seeds": "./seeds/",
		"resume": true
	}
}
