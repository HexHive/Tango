{
	"exec": {
		"path": "./daapd",
		"args": ["./daapd", "-d", "0", "-c", "forked-daapd.conf", "-f", "-w", "./htdocs"],
		"cwd": "./",
		"env": {
			"ASAN_OPTIONS": "abort_on_error=1:symbolize=0:detect_leaks=0",
			"LD_LIBRARY_PATH": "libevent_repo/build/lib"
		}
	},
	"channel": {
		"type": "tcp",
		"tcp": {
			"endpoint": "127.0.0.1",
			"port": 3689
		}
	},
	"loader": {
		"type": "replay",
		"forkserver": false,
		"disable_aslr": true
	},
	"input": {
		"type": "mutation"
	},
	"statemanager": {
		"type": "coverage",
		"strategy": "uniform"
	},
	"fuzzer": {
		"lib": "./lib/",
		"cwd": "./targets/daap/",
		"workdir": "./workdir/"
	}
}
