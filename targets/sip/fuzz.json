{
    "driver": {
        "type": "unix",
        "exec": {
            "path": "./kamailio",
            "args": ["./kamailio", "-f", "kamailio-basic.cfg", "-L", "./modules", "-Y", "./rundir", "-n", "1", "-D", "-E"],
            "cwd": "./",
            "env": {"ASAN_OPTIONS": "abort_on_error=1:symbolize=0:detect_leaks=0"}
        },
		"disable_aslr": true,
		"forkserver": true
    },
	"loader": {
		"type": "replay"
	},
	"channel": {
		"type": "udp",
        "endpoint": "127.0.0.1",
        "port": 5060
	},
	"generator": {
		"type": "reactive",
		"seeds": "./seeds/"
	},
	"tracker": {
		"type": "coverage"
	},
	"strategy": {
		"type": "uniform"
	},
	"fuzzer": {
		"cwd": "./targets/sip/",
		"work_dir": "./workdir/",
		"resume": true
	}
}
