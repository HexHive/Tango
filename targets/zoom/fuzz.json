{
	"exec": {
		"path": "./targets/zoom/repo/src/chocolate-doom",
		"args": ["./targets/zoom/repo/src/chocolate-doom", "-nomouse", "-nograbmouse", "-iwad", "Doom1.WAD", "-warp", "1", "-extraconfig", "custom.config", "-geometry", "1280x1024", "-nomusic", "-skill", "5"],
		"cwd": "./targets/zoom/repo/play",
		"env": {
		},
		"stdout": "./stdout",
		"stderr": "./stderr"
	},
	"channel": {
		"type": "x11"
	},
	"loader": {
		"type": "zoom",
		"forkserver": false,
		"disable_aslr": true
	},
	"input": {
		"type": "zoom"
	},
	"statemanager": {
		"type": "zoom",
		"strategy": "zoom",
		"validate_transitions": false,
		"minimize_transitions": false
	},
	"fuzzer": {
		"lib": "./lib",
		"workdir": "./workdir",
		"resume": true
	}
}
