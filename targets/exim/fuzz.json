{
	"driver": {
		"type": "unix",
		"exec": {
			"path": "./exim",
			"args": ["./exim", "-C", "exim.config", "-bdf", "-oX", "2025"],
			"cwd": "./",
			"env": {"ASAN_OPTIONS": "abort_on_error=1:symbolize=0:detect_leaks=0"}
		},
		"forkserver": true,
		"disable_aslr": true
	},
	"loader": {
		"type": "replay"
	},
	"channel": {
		"type": "tcp",
		"endpoint": "127.0.0.1",
		"port": 2025,
		"fork_before_accept": true
	},
	"generator": {
		"type": "reactive",
		"seeds": "./seeds/"
	},
	"tracker": {
		"type": "coverage"
	},
	"strategy": {
		"type": "uniform"
	},
	"fuzzer": {
		"cwd": "./targets/exim/",
		"work_dir": "./workdir/",
		"resume": true
	}
}
