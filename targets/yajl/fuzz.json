{
	"driver": {
		"type": "unix",
		"exec": {
			"path": "./parse",
			"args": ["./parse", "-b", "4"],
			"cwd": "./",
			"env": {
				"ASAN_OPTIONS": "abort_on_error=1:symbolize=0:detect_leaks=0"
			}
		},
		"forkserver": true,
		"disable_aslr": true
	},
	"loader": {
		"type": "replay"
	},
	"channel": {
		"type": "stdio"
	},
	"generator": {
		"type": "reactive",
		"chunk_size": 4
	},
	"tracker": {
		"type": "coverage"
	},
	"strategy": {
		"type": "uniform"
	},
	"fuzzer": {
		"cwd": "./targets/yajl/",
		"work_dir": "./workdir/",
		"resume": true
	}
}
