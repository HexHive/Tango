{
	"exec": {
		"path": "./parse",
		"args": ["./parse", "-b", "4"],
		"cwd": "./",
		"env": {
			"ASAN_OPTIONS": "abort_on_error=1:symbolize=0:detect_leaks=0"
		}
	},
	"channel": {
		"type": "stdio",
		"stdio": {
		}
	},
	"loader": {
		"type": "replay",
		"forkserver": true,
		"disable_aslr": true
	},
	"input": {
		"type": "reactive"
	},
	"statemanager": {
		"type": "coverage",
		"strategy": "uniform"
	},
	"fuzzer": {
		"lib": "./lib/",
		"cwd": "./targets/yajl/",
		"workdir": "./workdir/"
	}
}
