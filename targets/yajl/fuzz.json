{
	"loader": {
		"type": "replay",
		"exec": {
			"path": "./parse",
			"args": ["./parse", "-b", "4", "-p", "-m", "-c"],
			"cwd": "./",
			"env": {
				"ASAN_OPTIONS": "abort_on_error=1:symbolize=0:detect_leaks=0"
			}
		},
		"forkserver": true,
		"disable_aslr": true
	},
	"channel": {
		"type": "stdio"
	},
	"generator": {
		"type": "reactive"
	},
	"tracker": {
		"type": "coverage"
	},
	"strategy": {
		"type": "uniform"
	},
	"fuzzer": {
		"cwd": "./targets/yajl/",
		"work_dir": "./workdir/",
		"resume": true
	}
}
