{
	"exec": {
		"path": "./targets/forkexec_server/forkexec_server",
		"args": ["./targets/forkexec_server/forkexec_server"],
		"stdout": "./stdout",
		"stderr": "./stderr"
	},
	"channel": {
		"type": "tcp",
		"tcp": {
			"endpoint": "127.0.0.1",
			"port": 5001
		}
	},
	"loader": {
		"type": "replay",
		"forkserver": false,
		"disable_aslr": true
	},
	"input": {
		"type": "mutation"
	},
	"statemanager": {
		"type": "coverage"
	},
	"fuzzer": {
		"type": "coverage",
		"lib": "./lib",
		"workdir": "./workdir"
	}
}
