{
	"exec": {
		"path": "./forkexec_server",
		"args": ["./forkexec_server"],
		"stdout": "./stdout",
		"stderr": "./stderr"
	},
	"channel": {
		"type": "tcp",
		"tcp": {
			"endpoint": "127.0.0.1",
			"port": 5001
		}
	},
	"loader": {
		"type": "replay",
		"forkserver": true,
		"disable_aslr": true
	},
	"input": {
		"type": "mutation"
	},
	"statemanager": {
		"type": "coverage",
		"strategy": "uniform"
	},
	"fuzzer": {
		"cwd": "./targets/forkexec_server/",
		"lib": "./lib/",
		"workdir": "./workdir/"
	}
}
