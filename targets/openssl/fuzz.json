{
	"exec": {
		"path": "./openssl",
		"args": ["./openssl", "s_server", "-key", "key.pem", "-cert", "cert.pem", "-4", "-naccept", "1", "-no_anti_replay"],
		"cwd": "./",
		"env": {"ASAN_OPTIONS": "abort_on_error=1:symbolize=0:detect_leaks=0"}
	},
	"channel": {
		"type": "tcp",
		"tcp": {
			"endpoint": "127.0.0.1",
			"port": 4433
		}
	},
	"loader": {
		"type": "replay",
		"forkserver": true,
		"disable_aslr": true
	},
	"input": {
		"type": "reactive"
	},
	"statemanager": {
		"type": "coverage",
		"strategy": "uniform"
	},
	"fuzzer": {
		"lib": "./lib/",
		"cwd": "./targets/openssl/",
		"workdir": "./workdir/",
		"seeds": "./seeds/",
		"resume": true
	}
}
