{
	"exec": {
		"path": "./exim",
		"args": ["./exim", "-C", "exim.config", "-bdf", "-oX", "2025"],
		"cwd": "./",
		"env": {"ASAN_OPTIONS": "abort_on_error=1:symbolize=0:detect_leaks=0"}
	},
	"channel": {
		"type": "tcp",
		"tcp": {
			"endpoint": "127.0.0.1",
			"port": 2025,
			"fork_location": "accepting"
		}
	},
	"loader": {
		"type": "replay",
		"forkserver": true,
		"disable_aslr": true
	},
	"input": {
		"type": "reactive"
	},
	"statemanager": {
		"type": "coverage",
		"strategy": "uniform"
	},
	"fuzzer": {
		"lib": "./lib/",
		"cwd": "./targets/smtp/",
		"workdir": "./workdir/",
		"seeds": "./seeds/",
		"resume": true,
		"timescale": 0
	}
}
