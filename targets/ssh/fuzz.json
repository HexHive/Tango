{
	"exec": {
		"path": "./sshd",
		"args": ["./sshd", "-d", "-e", "-p", "2022", "-r", "-f", "sshd_config"],
		"cwd": "./",
		"env": {"ASAN_OPTIONS": "abort_on_error=1:symbolize=0:detect_leaks=0"}
	},
	"channel": {
		"type": "tcp",
		"tcp": {
			"endpoint": "127.0.0.1",
			"port": 2022
		}
	},
	"loader": {
		"type": "replay",
		"forkserver": true,
		"disable_aslr": true
	},
	"input": {
		"type": "mutation"
	},
	"statemanager": {
		"type": "coverage",
		"strategy": "uniform"
	},
	"fuzzer": {
		"lib": "./lib/",
		"cwd": "./targets/ssh/",
		"workdir": "./workdir/",
		"resume": true,
		"seeds": "./seeds/"
	}
}
