{
	"exec": {
		"path": "./dcmqrscp",
		"args": ["./dcmqrscp", "--single-process", "-c", "dcmqrscp.cfg"],
		"cwd": "./",
		"env": {
			"ASAN_OPTIONS": "abort_on_error=1:symbolize=0:detect_leaks=0",
			"DCMDICTPATH": "./dicom.dic"
		}
	},
	"channel": {
		"type": "tcp",
		"tcp": {
			"endpoint": "127.0.0.1",
			"port": 5158
		}
	},
	"loader": {
		"type": "replay",
		"forkserver": true,
		"disable_aslr": true
	},
	"input": {
		"type": "reactive"
	},
	"statemanager": {
		"type": "coverage",
		"strategy": "uniform"
	},
	"fuzzer": {
		"cwd": "./targets/dcmtk/",
		"lib": "./lib/",
		"seeds": "./seeds/",
		"workdir": "./workdir/",
		"resume": true
	}
}
