{
	"driver": {
		"type": "unix",
		"exec": {
			"path": "./xmlwf",
			"args": ["./xmlwf"],
			"cwd": "./",
			"env": {
				"ASAN_OPTIONS": "abort_on_error=1:symbolize=0:detect_leaks=0",
				"LD_LIBRARY_PATH": "libexpat_repo/expat/lib/.libs/"
			}
		},
		"forkserver": true,
		"disable_aslr": true
	},
	"loader": {
		"type": "replay"
	},
	"channel": {
		"type": "stdio"
	},
	"generator": {
		"type": "reactive",
        "seeds": "./seeds/"
	},
	"tracker": {
		"type": "coverage"
	},
	"strategy": {
		"type": "uniform"
	},
	"fuzzer": {
		"cwd": "./targets/expat/",
		"work_dir": "./workdir/",
		"resume": true
	}
}
